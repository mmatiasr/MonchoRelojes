<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
<servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>tienda.Login</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login.html</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>Registro</servlet-name>
    <servlet-class>tienda.Registro</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>Registro</servlet-name>
    <url-pattern>/registro.html</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>tienda.Logout</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout.html</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ProcesarPedido</servlet-name>
    <servlet-class>tienda.ProcesarPedido</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>ProcesarPedido</servlet-name>
    <url-pattern>/ProcesarPedido.html</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ActualizarUsuario</servlet-name>
    <servlet-class>tienda.ActualizarUsuario</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>ActualizarUsuario</servlet-name>
    <url-pattern>/ActualizarUsuario.html</url-pattern>
</servlet-mapping>
<servlet>
  <servlet-name>CancelarPedido</servlet-name>
  <servlet-class>tienda.CancelarPedido</servlet-class>
</servlet>

<servlet-mapping>
  <servlet-name>CancelarPedido</servlet-name>
  <url-pattern>/CancelarPedido.html</url-pattern>
</servlet-mapping>

</web-app>
